  "dns": {
    "independent_cache": true,
    "rules": [
      {
        "outbound": "any",
        "server": "local"
      },
      {
        "query_type": [
          "AAAA",
          "A"
        ],
        "server": "fakeip"
      }
    ],
    "servers": [
      {
        "server": "1.1.1.2",
        "tag": "remote",
        "type": "https"
      },
      {
        "detour": "direct",
        "tag": "local",
        "type": "local"
      },
      {
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18",
        "tag": "fakeip",
        "type": "fakeip"
      }
    ],
    "strategy": "prefer_ipv6"
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
