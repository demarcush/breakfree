          "enabled": true,
          "server": "localhost",
          "server_port": 7890
        }
      },
      "stack": "system",
      "strict_route": true,
      "type": "tun"
    },
    {
      "listen": "::",
      "listen_port": 7890,
      "type": "mixed"
    }
  ],
  "log": {
    "level": "fatal"
  },
  "outbounds": [
