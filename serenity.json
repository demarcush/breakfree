{
  "log": {
    "level": "debug"
  },
  "listen": "127.0.0.1:8080",
  "cache_file": "serenity.db",
  "outbounds": [
    {
      "type": "direct"
    },
    {
      "type": "dns"
    }
  ],
  "subscriptions": [
    {
      "name": "azadi",
      "url": "https://cdn.jsdelivr.net/gh/azadiazinjamigzare/Service@main/Sub"
    },
    {
      "name": "ircp",
      "url": "https://cdn.jsdelivr.net/gh/IranianCypherpunks/SingBox@main/Sub"
    },
    {
      "name": "tvc",
      "url": "https://cdn.jsdelivr.net/gh/yebekhe/TVC@main/subscriptions/singbox/mix.json"
    },
    {
      "name": "eternity",
      "url": "https://cdn.jsdelivr.net/gh/mahdibland/V2RayAggregator@master/sub/sub_merge_yaml.yml"
    },
    {
      "name": "collector",
      "url": "https://cdn.jsdelivr.net/gh/Mahdi0024/ProxyCollector@master/sub/singbox.json"
    },
    {
      "name": "final",
      "url": "http://127.0.0.1:8080/servers",
      "update_interval": "5m0s",
      "process": {
        "filter_type": [
          "http",
          "shadowsocks",
          "shadowsocksr",
          "socks"
        ],
        "remove": true
      },
      "deduplication": true
    }
  ],
  "templates": [
    {
      "name": "breakfree"
    }
  ],
  "profiles": [
    {
      "name": "servers",
      "template": "breakfree",
      "subscription": [
        "azadi",
        "ircp",
        "tvc",
        "eternity"
      ]
    },
    {
      "name": "final",
      "template": "breakfree",
      "subscription": "final"
    }
  ]
}
